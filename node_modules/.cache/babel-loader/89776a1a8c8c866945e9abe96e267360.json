ttp://localhost:8222/acl/menu/treeList',\n                method: 'get',\n            })\n            .then((response) => {\n                // 在这里处理响应数据\n                // console.log('响应数据：', response.data)\n                // 可以在这里进行进一步的操作，例如将数据存储到 Vuex store 或更新组件的数据\n                // 将响应数据传递给调用方\n                resolve(response.data)\n            })\n            .catch((error) => {\n                // 处理错误情况\n                console.error('请求错误：', error)\n                    // 将错误信息传递给调用方\n                reject(error)\n            })\n    })\n}"],"mappings":"AAAA;AACA,OAAOA,OAAO,MAAM,iBAAiB;;AAErC;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASC,KAAKA,CAACC,QAAQ,EAAEC,QAAQ,EAAE;EACtC;EACA,OAAOH,OAAO,CAAC;IACXI,GAAG,EAAE,uCAAuC;IAC5CC,MAAM,EAAE,MAAM;IACdC,IAAI,EAAE;MACFJ,QAAQ;MACRC;IACJ;EACJ,CAAC,CAAC,CAACI,KAAK,CAAEC,KAAK,IAAK;IAChBC,OAAO,CAACD,KAAK,CAAC,MAAM,EAAEA,KAAK,CAAC;IAC5B,MAAMA,KAAK;EACf,CAAC,CAAC;AACN;;AAEA;AACA,OAAO,SAASE,OAAOA,CAAA,EAAG;EACtB,OAAOV,OAAO,CAAC;IACXI,GAAG,EAAE,sCAAsC;IAC3CC,MAAM,EAAE;EACZ,CAAC,CAAC;AACN;;AAEA;AACA,OAAO,SAASM,MAAMA,CAAA,EAAG;EACrB,OAAOX,OAAO,CAAC;IACXI,GAAG,EAAE,wCAAwC;IAC7CC,MAAM,EAAE;EACZ,CAAC,CAAC;AACN;;AAEA;AACA,OAAO,SAASO,OAAOA,CAAA,EAAG;EACtB,OAAO,IAAIC,OAAO,CAAC,CAACC,OAAO,EAAEC,MAAM,KAAK;IACpCf,OAAO,CAAC;MACAI,GAAG,EAAE,yCAAyC;MAC9CC,MAAM,EAAE;IACZ,CAAC,CAAC,CACDW,IAAI,CAAEC,QAAQ,IAAK;MAChB;MACA;MACA;MACA;MACAH,OAAO,CAACG,QAAQ,CAACX,IAAI,CAAC;IAC1B,CAAC,CAAC,CACDC,KAAK,CAAEC,KAAK,IAAK;MACd;MACAC,OAAO,CAACD,KAAK,CAAC,OAAO,EAAEA,KAAK,CAAC;MACzB;MACJO,MAAM,CAACP,KAAK,CAAC;IACjB,CAAC,CAAC;EACV,CAAC,CAAC;AACN"}]}