-9.,?'\\\\+&%$#=~_-]+))*$/\n    return urlregex.test(textval)\n}\n\n/* 小写字母*/\nexport function validateLowerCase(str) {\n    const reg = /^[a-z]+$/\n    return reg.test(str)\n}\n\n/* 大写字母*/\nexport function validateUpperCase(str) {\n    const reg = /^[A-Z]+$/\n    return reg.test(str)\n}\n\n/* 大小写字母*/\nexport function validatAlphabets(str) {\n    const reg = /^[A-Za-z]+$/\n    return reg.test(str)\n}"],"mappings":"AAAA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA,OAAO,SAASA,UAAUA,CAACC,IAAI,EAAE;EAC7B,OAAO,yBAAyB,CAACC,IAAI,CAACD,IAAI,CAAC;AAC/C;;AAEA;AACA;AACA;AACA;AACA,OAAO,SAASE,aAAaA,CAACC,GAAG,EAAE;EAC/B,MAAMC,SAAS,GAAG,CAAC,OAAO,EAAE,QAAQ,CAAC;EACrC,OAAOA,SAAS,CAACC,OAAO,CAACF,GAAG,CAACG,IAAI,EAAE,CAAC,IAAI,CAAC;AAC7C;;AAEA;AACA;AACA;AACA;;AAEA,OAAO,SAASC,eAAeA,CAACJ,GAAG,EAAE;EACjC;EACA;EACA,OAAOA,GAAG,CAACG,IAAI,EAAE,KAAK,EAAE;AAC5B;;AAEA;AACA,OAAO,SAASE,WAAWA,CAACC,OAAO,EAAE;EACjC,MAAMC,QAAQ,GAAG,4TAA4T;EAC7U,OAAOA,QAAQ,CAACT,IAAI,CAACQ,OAAO,CAAC;AACjC;;AAEA;AACA,OAAO,SAASE,iBAAiBA,CAACR,GAAG,EAAE;EACnC,MAAMS,GAAG,GAAG,UAAU;EACtB,OAAOA,GAAG,CAACX,IAAI,CAACE,GAAG,CAAC;AACxB;;AAEA;AACA,OAAO,SAASU,iBAAiBA,CAACV,GAAG,EAAE;EACnC,MAAMS,GAAG,GAAG,UAAU;EACtB,OAAOA,GAAG,CAACX,IAAI,CAACE,GAAG,CAAC;AACxB;;AAEA;AACA,OAAO,SAASW,gBAAgBA,CAACX,GAAG,EAAE;EAClC,MAAMS,GAAG,GAAG,aAAa;EACzB,OAAOA,GAAG,CAACX,IAAI,CAACE,GAAG,CAAC;AACxB"}]}